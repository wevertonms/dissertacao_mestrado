{
  "cSpell.language": "pt-BR,en",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.aux": true,
    "**/*.lof": true,
    "**/*.log": true,
    "**/*.lot": true,
    "**/*.lol": true,
    "**/*.fls": true,
    "**/*.out": true,
    "**/*.toc": true,
    "**/*.fmt": true,
    "**/*.fot": true,
    "**/*.cb": true,
    "**/*.cb2": true,
    "**/.*.lb": true,
    "**/*.idx": true,
    "**/*.ps": true,
    "**/*.ind": true,
    "**/*.ilg": true,
    "**/__*": true,
    "**/*.dvi": true,
    "**/*.xdv": true,
    "**/*.bbl": true,
    "**/*.bcf": true,
    "**/*.blg": true,
    "**/*-blx.aux": true,
    "**/*-blx.bib": true,
    "**/*.brf": true,
    "**/*.run.xml": true,
    "**/*.synctex": true,
    "**/*.synctex(busy)": true,
    "**/*.synctex.gz": true,
    "**/*.synctex.gz(busy)": true,
    "**/*_latexmk": true,
    "**/.directory": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "cSpell.words": [
    "ABAQUS",
    "ANSYS",
    "Bokeh",
    "Bèzier",
    "COEFF",
    "DNVGL",
    "ELEMENTSET",
    "FATFREE",
    "Fugro",
    "HMTL",
    "ISIGHT",
    "Indenta",
    "Keulegan",
    "Kármán",
    "LKCR",
    "Matplotlib",
    "Multimode",
    "NODESET",
    "Numpy",
    "SEABEDSET",
    "Sesam",
    "Strouhal",
    "Subsea",
    "Swartz",
    "abntex",
    "adimensionalidos",
    "adimensionalizada",
    "backgroundcolor",
    "breaklines",
    "captionof",
    "chktex",
    "delim",
    "dissertacao",
    "elset",
    "ficos",
    "fısico",
    "hyperpageref",
    "includepdf",
    "indentação",
    "integrispan",
    "interar",
    "metaoceanogr",
    "metaoceanográficos",
    "numbersep",
    "numberstyle",
    "partapendices",
    "partpage",
    "showstringspaces",
    "stepnumber",
    "tdidf",
    "xlwings"
  ],
  "editor.wordWrap": "on",
  "latex-workshop.chktex.run": "onSave",
  "latex-workshop.latex.build.forceRecipeUsage": true,
  "latex-workshop.latex.recipe.default": "lastUsed",
  "latex-workshop.synctex.afterBuild.enabled": true,
  "editor.formatOnSave": false,
  "editor.fontSize": 16,
  "latex-workshop.latex.external.build.args": [
    "-shell-escape"
  ],
  "latex-workshop.latex.tools": [
   {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-shell-escape",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": [
        "%DOCFILE%"
      ],
      "env": {}
    },
  ],
  "latex-workshop.chktex.enabled": true,
  "latex-workshop.chktex.args.active": [
    "-wall",
    "-n22",
    "-n30",
    "-e16",
    "-q",
    "-n18",
    "-n46",
    "-n1",
    "-n13",
    "-n36"
  ],
  "todo-tree.regex.regex": "((//|#|<!--|;|/\\*|^|%|\\\\)\\s*($TAGS)\\{*|^\\s*- \\[ \\])",
  "todo-tree.filtering.includeGlobs": [
    "**/*.tex",
  ],
  "ltex.additionalRules.motherTongue": "pt-BR",
  "ltex.checkFrequency": "save",
  "ltex.enabled": false,
  "ltex.language": "pt-BR",
  "window.zoomLevel": 1,
  "python.testing.promptToConfigure": false,
  "python.testing.pytestEnabled": false,
  "python.testing.unittestEnabled": false,
  "python.testing.nosetestsEnabled": false,
  "latex-workshop.latex.autoBuild.run": "onSave",
  "gitdoc.enabled": false,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#8a2ca2",
    "activityBar.activeBorder": "#af9530",
    "activityBar.background": "#8a2ca2",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#af9530",
    "activityBarBadge.foreground": "#15202b",
    "statusBar.background": "#68217a",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#8a2ca2",
    "titleBar.activeBackground": "#68217a",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#68217a99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#68217A",
}