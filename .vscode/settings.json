{
  "cSpell.language": "pt-BR,en",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.aux": true,
    "**/*.lof": true,
    "**/*.log": true,
    "**/*.lot": true,
    "**/*.fls": true,
    "**/*.out": true,
    "**/*.toc": true,
    "**/*.fmt": true,
    "**/*.fot": true,
    "**/*.cb": true,
    "**/*.cb2": true,
    "**/.*.lb": true,
    "**/*.idx": true,
    "**/*.ps": true,
    "**/*.ind": true,
    "**/*.ilg": true,
    "**/__*": true,
    "**/*.dvi": true,
    "**/*.xdv": true,
    "**/*.bbl": true,
    "**/*.bcf": true,
    "**/*.blg": true,
    "**/*-blx.aux": true,
    "**/*-blx.bib": true,
    "**/*.brf": true,
    "**/*.run.xml": true,
    "**/*.synctex": true,
    "**/*.synctex(busy)": true,
    "**/*.synctex.gz": true,
    "**/*.synctex.gz(busy)": true,
    "**/*_latexmk": true,
    "**/.directory": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "cSpell.words": [
    "Abaqus",
    "Bokeh",
    "Bèzier",
    "DNVGL",
    "Indenta",
    "Keulegan",
    "Kármán",
    "LKCR",
    "Matplotlib",
    "Numpy",
    "Strouhal",
    "Subsea",
    "abntex",
    "backgroundcolor",
    "breaklines",
    "delim",
    "fısico",
    "hyperpageref",
    "indentação",
    "interar",
    "numbersep",
    "numberstyle",
    "showstringspaces",
    "stepnumber",
    "xlwings"
  ],
  "editor.wordWrap": "on",
  "latex-workshop.chktex.run": "onSave",
  "latex-workshop.chktex.args.active": [
    "-wall",
    "-n22",
    "-n30",
    "-n1",
    "-e16",
    "-q"
  ],
  "latex.build.args": [
    "-pdf",
    "-interaction=nonstopmode",
    "-synctex=1",
    "./dissertação.tex"
  ],
  "latex-workshop.latex.autoBuild.run": "onFileChange",
  "latex-workshop.latex.build.forceRecipeUsage": true,
  "latex-workshop.latex.recipe.default": "lastUsed",
  "latex-workshop.synctex.afterBuild.enabled": true
}
